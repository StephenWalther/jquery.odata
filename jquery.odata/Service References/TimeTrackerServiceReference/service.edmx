      </EntityType>
      <Association Name="ItemUser">
        <End Role="Item" Type="TimeTracker.Item" Multiplicity="*" />
        <End Role="User" Type="TimeTracker.User" Multiplicity="1" />
        <ReferentialConstraint>
          <Principal Role="User">
            <PropertyRef Name="Id" />
          </Principal>
          <Dependent Role="Item">
            <PropertyRef Name="UserId" />
          </Dependent>
        </ReferentialConstraint>
      </Association>
      <Association Name="ItemCategory">
